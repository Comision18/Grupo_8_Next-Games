<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" sizes="any" href="/images/next-games-nuevologo.png">
    <!-- fontawesome -->
    <script src="https://kit.fontawesome.com/2d353964a9.js" crossorigin="anonymous"></script>
    
    
    <link rel="stylesheet" href="/stylesheets/style.css">
    <title>Next Game | Usuarios</title>
</head>


<body>
    <%- include('partials/header') %> 
    
        <div class="home__main--titulo">
            <h2>Todos los usuarios</h2>
        </div>

        <section class="home__main__section">
			<% users.forEach(({id, firtsName, lastName, email, password, image}) => { %>
				<%- include('./users/register',{
					id,
					firtsName,
					lastName,
					email,
					name,
                    password,
                    image
				}) %>
			<% }) %>
        </section>

         <!--whatsapp-->
         <%- include('./partials/whatsApp.ejs') %>
       
    <%- include('partials/footer') %>
</body>
</html>